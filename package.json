{
  "name": "gems",
  "version": "0.1.0",
  "description": "Gem maker",
  "license": "MIT",
  "engines": {
    "node": "4.x"
  },
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "lint": "npm run lint-js && npm run lint-css",
    "lint-css": "stylelint './src/**/*.css'",
    "lint-js": "eslint .",
    "postinstall": "npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "deploy": "bash deploy.sh"
  },
  "nyc": {
    "exclude": [
      "**/*.test.js",
      "test/entry.js",
      "server/*",
      "dist/*"
    ]
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "autoprefixer-core": "6.0.1",
    "babel": "6.5.2",
    "babel-cli": "6.7.5",
    "babel-core": "6.4.0",
    "babel-eslint": "6.0.2",
    "babel-loader": "6.2.1",
    "babel-plugin-__coverage__": "0.1111.1",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "6.7.2",
    "color": "0.11.1",
    "cross-env": "1.0.7",
    "css-loader": "0.23.0",
    "eslint": "2.7.0",
    "eslint-loader": "1.1.1",
    "eslint-plugin-react": "4.2.0",
    "file-loader": "0.8.4",
    "html-webpack-plugin": "2.9.0",
    "immutable": "3.7.5",
    "postcss-cssnext": "2.4.0",
    "postcss-import": "8.0.2",
    "postcss-loader": "0.8.0",
    "react": "15.0.0",
    "react-dom": "15.0.0",
    "react-hot-loader": "1.3.0",
    "react-redux": "4.4.2",
    "redux": "3.3.1",
    "redux-logger": "2.1.4",
    "reselect": "2.5.1",
    "rimraf": "2.4.3",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.0",
    "stylelint": "5.2.1",
    "url-loader": "0.5.6",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.14.1",
    "webpack-hot-middleware": "2.6.4"
  },
  "dependencies": {
    "express": "4.13.3",
    "helmet": "1.1.0",
    "http-proxy": "1.12.1",
    "winston": "2.1.1"
  },
  "keywords": [
    "gems",
    "svg",
    "react",
    "redux",
    "lighting"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/winkerVSbecks/gems.git"
  },
  "bugs": {
    "url": "https://github.com/winkerVSbecks/gems/issues"
  }
}
